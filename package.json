		"tailwindcss": "^3.4.1",
		"tsafe": "^1.6.6",
		"tslib": "^2.6.2",
		"typescript": "5.1.6",
		"vite": "^4.5.3",
		"vite-plugin-iso-import": "^1.1.0",
		"vitest": "^0.32.4"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"pnpm": {
		"overrides": {
			"protobufjs@>=6.10.0 <7.2.4": ">=7.2.4",
			"follow-redirects@<1.15.4": ">=1.15.4",
			"@adobe/css-tools@<4.3.2": ">=4.3.2",
